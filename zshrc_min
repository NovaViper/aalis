# If not running interactively, don't do anything
[[ $- != *i* ]] && return

neofetch --ascii_distro arch_small

#### 
# User Configurations
####

HISTFILE=~/.config/zsh/.zsh_history
HISTSIZE=1000
SAVEHIST=1000

#Enable AutoCD into dir, terminal bell, and terminal notify
setopt autocd beep notify

# Enable Vi mode
bindkey -v